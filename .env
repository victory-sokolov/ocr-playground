ENV=local
OCR_ENGINE=Tesseract

# Upload files settings
UPLOAD_FOLDER=static

# Tesseract configs
LOAD_FREQ_DAWG=0
LOAD_PUNC_DAWG=0
LOAD_SYSTEM_DAWG=0

# Redis
REDIS_VERSION=7.2.3

# Database
POSTGRES_VERSION=15.5
POSTGRES_PASSWORD="fastapi"
POSTGRES_USER="fastapi"
POSTGRES_DB="appocr"
WRITER_DB_URL="postgresql+asyncpg://${POSTGRES_USER}:${POSTGRES_PASSWORD}@0.0.0.0:5432/${POSTGRES_DB}"
READER_DB_URL="postgresql+asyncpg://${POSTGRES_USER}:${POSTGRES_PASSWORD}@0.0.0.0:5432/${POSTGRES_DB}"
